<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd"> -->
<module rename-to='VkGraph'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- Other module inherits -->
	<source path='client' />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name='com.google.gwt.junit.JUnit' />

	<!-- CSS files -->

	<stylesheet src="jqplot/jquery.jqplot.css" />
	<stylesheet src="jquery/smoothness/jquery-ui-1.8.23.custom.css" />

	<!-- JQuery dependence -->
	<script src="jqplot/jquery.min.js"></script>
	<script src="jqplot/jquery.sideswap.js"></script>
	<!-- JQplot dependence -->
	<script src="jqplot/jquery.jqplot.min.js"></script>
	<script src="jqplot/chartsLoading.js"></script>
	<!-- JQuery dependence -->
	<script src="jquery/jquery-ui-1.8.23.custom.min.js"></script>

	<!--[if lt IE 9]><script language="javascript" src="jqplot/excanvas.js"></script><![endif] -->

	<!-- PLUG-IN JQplot -->
	<!-- Axes -->
	<script src="jqplot/plugins/jqplot.dateAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.logAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.canvasAxisLabelRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.categoryAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.canvasAxisTickRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.canvasTextRenderer.min.js"></script>

	<!-- Cursor -->
	<script src="jqplot/plugins/jqplot.cursor.min.js"></script>
	<!-- Highlighter -->
	<script src="jqplot/plugins/jqplot.highlighter.min.js"></script>
	<!-- Drag-drop -->
	<script src="jqplot/plugins/jqplot.dragable.min.js"></script>
	<!-- Trendline -->
	<script src="jqplot/plugins/jqplot.trendline.min.js"></script>


	<!-- Charts types -->
	<!-- Pies -->
	<script src="jqplot/plugins/jqplot.pieRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.donutRenderer.min.js"></script>
	<!-- Block plot -->
	<script src="jqplot/plugins/jqplot.blockRenderer.min.js"></script>
	<!-- Pyramid -->
	<script src="jqplot/plugins/jqplot.pyramidRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.pyramidAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/jqplot.pyramidGridRenderer.min.js"></script>
	<!-- Bubble plot -->
	<script src="jqplot/plugins/jqplot.bubbleRenderer.min.js"></script>
	<!-- OHLC plot -->
	<script src="jqplot/plugins/jqplot.ohlcRenderer.js"></script>

	<!-- Bar chart -->
	<script src="jqplot/plugins/jqplot.barRenderer.min.js"></script>
	<!-- Bezier chart -->
	<script src="jqplot/plugins/jqplot.BezierCurveRenderer.min.js"></script>
	<!-- Gauge chart -->
	<script src="jqplot/plugins/jqplot.meterGaugeRenderer.min.js"></script>

	<!-- Horizontal / vertical lines -->
	<script src="jqplot/plugins/jqplot.canvasOverlay.min.js"></script>
	<!-- Legend dynamic -->
	<script src="jqplot/plugins/jqplot.enhancedLegendRenderer.min.js"></script>

	<!-- Customization theme -->
	<script src="fr/vekia/js/extendedThemes.js"></script>
	<!-- Export PNG -->
	<script src="fr/vekia/js/exportImg.js"></script>

	<!-- Slider on showcase presentation page -->
	<script src="jquery/jquery.lite-content-slider.min.js"></script>
</module>
