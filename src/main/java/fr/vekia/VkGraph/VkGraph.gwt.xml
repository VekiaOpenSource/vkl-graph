<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd"> -->
<module rename-to='VkGraph'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- Other module inherits -->
	<source path='client' />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name='com.google.gwt.junit.JUnit' />

	<!-- CSS files -->
	<stylesheet src="jqplot/core/jquery.jqplot.min.css" />

	<!-- JQuery dependence -->
	<script src="jqplot/core/jquery.min.js"></script>
	<script src="jqplot/core/jquery.jqplot.min.js"></script>

	<!-- PLUG-IN JQplot -->
	<!-- Axes -->
	<script src="jqplot/plugins/axis/jqplot.dateAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/axis/jqplot.logAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/axis/jqplot.canvasAxisLabelRenderer.min.js"></script>
	<script src="jqplot/plugins/axis/jqplot.categoryAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/axis/jqplot.canvasAxisTickRenderer.min.js"></script>
	<script src="jqplot/plugins/text/jqplot.canvasTextRenderer.min.js"></script>

	<!-- Cursor -->
	<script src="jqplot/plugins/cursor/jqplot.cursor.min.js"></script>
	<!-- Highlighter -->
	<script src="jqplot/plugins/highlighter/jqplot.highlighter.min.js"></script>
	<!-- Drag-drop -->
	<script src="jqplot/plugins/dnd/jqplot.dragable.min.js"></script>
	<!-- Trendline -->
	<script src="jqplot/plugins/trendline/jqplot.trendline.min.js"></script>


	<!-- Charts types -->
	<!-- Pies -->
	<script src="jqplot/plugins/pie/jqplot.pieRenderer.min.js"></script>
	<script src="jqplot/plugins/donut/jqplot.donutRenderer.min.js"></script>
	<!-- Block plot -->
	<script src="jqplot/plugins/block/jqplot.blockRenderer.min.js"></script>
	<!-- Pyramid -->
	<script src="jqplot/plugins/pyramid/jqplot.pyramidRenderer.min.js"></script>
	<script src="jqplot/plugins/pyramid/jqplot.pyramidAxisRenderer.min.js"></script>
	<script src="jqplot/plugins/pyramid/jqplot.pyramidGridRenderer.min.js"></script>
	<!-- Bubble plot -->
	<script src="jqplot/plugins/bubble/jqplot.bubbleRenderer.min.js"></script>
	<!-- OHLC plot -->
	<script src="jqplot/plugins/ohlc/jqplot.ohlcRenderer.js"></script>

	<!-- Bar chart -->
	<script src="jqplot/plugins/bar/jqplot.barRenderer.min.js"></script>
	<!-- Bezier chart -->
	<script src="jqplot/plugins/bezier/jqplot.BezierCurveRenderer.min.js"></script>
	<!-- Gauge chart -->
	<script src="jqplot/plugins/gauge/jqplot.meterGaugeRenderer.min.js"></script>

	<!-- Horizontal / vertical lines -->
	<script src="jqplot/plugins/overlay/jqplot.canvasOverlay.min.js"></script>
	<!-- Legend dynamic -->
	<script src="jqplot/plugins/legend/jqplot.enhancedLegendRenderer.min.js"></script>
</module>
